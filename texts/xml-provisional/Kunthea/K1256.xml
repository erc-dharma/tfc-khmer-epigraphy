<lb n="B21"/>ka<unclear>k</unclear>nac· sanre <num value="2"><g type="numeral">II</g></num> tmur· <num value="20">bhaiḥ</num> <num value="7"><g type="numeral">7</g></num> krapi <num value="12"><g type="numeral">10</g><g type="numeral">II</g></num>

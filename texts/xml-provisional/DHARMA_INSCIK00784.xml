                      <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/> k<unclear cert="low">ā</unclear><gap reason="lost" quantity="1" unit="character"/> <num value="1">I</num> ku Āka<unclear>ṁre</unclear>m· <num value="1">I</num> ku <unclear>roy</unclear>· <num value="1">I</num> ku caṅ·hvā<unclear>r</unclear>· <num value="1">I</num> ku <gap reason="lost" quantity="1" unit="character"/>
                      <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/> ku <gap reason="lost" quantity="1" unit="character"/> <num value="1">I</num> ku <gap reason="lost" quantity="1" unit="character"/>
                      <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/> ku <gap reason="lost" quantity="1" unit="character"/> <num value="1">I</num> kon ku k<unclear>u</unclear> <gap reason="lost" extent="unknown" unit="character"/> <num value="1">I</num> ku <unclear>tva</unclear>ṅ· <num value="1">I</num>

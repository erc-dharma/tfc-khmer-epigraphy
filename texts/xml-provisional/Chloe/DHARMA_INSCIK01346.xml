         <div type="edition" xml:lang="san-Latn">
         	<lg n="1" met="anuṣṭubh">
         	    <l n="a"><lb n="1"/>namāmy annābhi-padma-stho <space/></l>
         	    <l n="b">yasya padmodbhavoddhruvam·</l>
         	    <l n="c"><lb n="2"/>catur-vvāhu-guṇaṁ vaktuṁ <space/></l>
         	    <l n="d">bhūri-vaddha-catur-mmukhaḥ <g type="danda">.</g></l>
         	</lg>
             <lg n="2" met="anuṣṭubh">
                  <l n="a"><lb n="3"/>mahā-bhuje bhima-bhujaṅga-śobhe <space/></l>
                 <l n="b"></l>
                 <l n="c"><lb n="4"/>viddhāya ?ryyapratitaṁ yaśo</l>
                 <l n="d">śauriḥ purā śo..satan vande <g type="danda">.</g></l>
             </lg>
             <lg n="2" met="anuṣṭubh">
                  <l n="a"><lb n="5"/> <space/></l>
                 <l n="b">bhuti/vi sindhi.obhaa</l>
                 <l n="c"><lb n="6"/></l>
                 <l n="d"> <g type="danda">.</g></l>
                ta pi  paripala n
                nandizura
             </lg>
         </div>
         <div type="apparatus">
         </div>

   <lb n="C20"/><gap reason="lost" quantity="9" unit="character"/>
   <lb n="C21"/><gap reason="lost" quantity="10" unit="character"/>
   <lb n="C22"/><gap reason="lost" extent="unknown" unit="character"/>
   <lb n="C23"/><gap reason="lost" extent="unknown" unit="character"/>
   
</p>

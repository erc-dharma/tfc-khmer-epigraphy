                <div type="edition" xml:lang="okz-Latn" rendition="class:83231 maturity:83211">
                
<lg n="1" met="pr̥thvī" xml:lang="san-Latn"><milestone type="pagelike" unit="item" n="S"/><label xml:lang="eng">Southern Doorjamb</label>
<lb n="S1"/><l n="a">jagat-sakala-saṁbhava-sthiti-nirodha-karttā prabhuḥ<space/></l>
    <l n="b">prajāpati-harīndra-deva-muni-daitya-siddhais stutaḥ</l>
<l n="c">dhiyām adhipat<supplied reason="lost">i</supplied>-<seg met="-+---+-++-+"><gap reason="lost" quantity="11" unit="character"/></seg></l>
    <l n="d"><lb n="S2"/><supplied reason="lost">ja</supplied>yaty amala-candra-maulir umayā saha yaś śivaḥ <g type="ddanda">.</g></l>
</lg>
<lg n="2" met="upajāti" xml:lang="san-Latn">
   <l n="a">jayaty asahyādbhuta-śakti-vīryyas<space/></l>
<l n="b">samudra-paryyanta-yaśo-vitānaḥ</l>
<l n="c"><seg met="=+-++--"><gap reason="lost" quantity="7" unit="character"/></seg><lb n="S3"/> mā ù ratnaḥ</l>
<l n="d">dvirepha-saṁspr̥ṣṭa-<unclear>pa</unclear>dāravindaḥ <g type="ddanda">.</g></l>
</lg>
                    <lg n="3" met="upajāti" xml:lang="san-Latn">
                       <l n="a">sūryyendu-devendra-manoja-tulyaḥ<space/></l>
<l n="b">prabhāvakān<unclear>t</unclear>itva-vibhūti-r<supplied reason="lost">ū</supplied>paiḥ<space/></l>
<l n="c">śauryya-<seg met="="><gap reason="lost" quantity="1" unit="character"/></seg>-tejo<seg met="--+"><gap reason="lost" quantity="3" unit="character"/></seg><lb n="S4"/>s sa <seg met="+"><gap reason="lost" quantity="1" unit="character"/></seg>ntai<space/>r</l>
<l n="d">upendra-vitteśvara-vāyu-mitraiḥ <g type="ddanda">.</g></l>
</lg>
                    <lg n="4" met="upajāti" xml:lang="san-Latn">
                       <l n="a">bhūmīśvarāṇāṁ bhuvi yasya tuṅgai<space/>ś</l>
<l n="b">śirobhir ājñā srag ivoddhr̥tā yā<space/></l>
<l n="c">saś śrīnivāso jayava<lb n="S5"/><supplied reason="lost">rmma</supplied><unclear>devo</unclear></l>
<l n="d">divīva śakrasya surāsurāṇāṁ <g type="ddanda">.</g></l>
</lg>
                    <lg n="5" met="anuṣṭubh" xml:lang="san-Latn">
                       <l n="a" real="-+-++++=">praśāsatīmān dharmmeṇa<space/></l>
<l n="b">vasudhāṁ vasudhādhipe<space/></l>
<l n="c">samudra-jala-saṁvītāṁ<space/></l>
<l n="d">himādryānaddha-mekhalā<unclear>m·</unclear> <g type="ddanda">.</g></l>
</lg>
                    <lg n="6" met="anuṣṭubh" xml:lang="san-Latn">
                       <lb n="S6"/><l n="a"><seg met="=="><gap reason="lost" quantity="2" unit="character"/></seg>dvijarṣabhenaiva<space/></l>
<l n="b">śrīkurukṣetra-vāsinā<space/></l>
<l n="c">yajvanā śrotriyais sarvvai<space/>r</l>
<l n="d">ukkhalādityasvāminā <g type="ddanda">.</g></l>
</lg>
                    <lg n="7" met="anuṣṭubh" xml:lang="san-Latn">
                       <l n="a">sthāpito vidhinā devaḥ<space/></l>
<lb n="S1"/><l n="b"><unclear>śrī</unclear>sthāneśvara-saṁjñakaḥ<space/></l>
<l n="c" enjamb="yes">bhūmi-kṣetra-vanārāma</l>
<l n="d">-bhr̥tyavarggāṁs tathaiva gāḥ <g type="ddanda">.</g></l>
</lg>
                    <lg n="8" met="upajāti" xml:lang="san-Latn">
                       <l n="a">sahito dāra-putrādyai<space/>ś</l>
<l n="b">śraddhāyuktaiś ca dhārmmikaiḥ<space/></l>
<lb n="S8"/><l n="c"><unclear>yaś śrī</unclear>sthāneśvarāyainā<space/>n</l>
<l n="d">dadau tasmāy aśeṣataḥ <g type="ddanda">.</g></l>
</lg>
                    <lg n="9" met="śārdūlavikrīḍita" xml:lang="san-Latn">
                       <l n="a">bhuktā ṣaṭr̥ṣi-pāṇḍavaiś śakaśarad vaiśākha-māse yadā<space/></l>
<l n="b">kr̥ṣṇaikādaśi-saṁyute <lb n="S9"/><seg met="-"><gap reason="lost" quantity="1" unit="character"/></seg><unclear>ka</unclear>-suto lagne <seg met="-"><gap reason="lost" quantity="1" unit="character"/></seg> yugmāṅkite<space/></l>
<l n="c">Aśvinyāṁ śaśa-bhr̥d vr̥ṣe ravi-vudhau siṅhe varājārkkajau</l>
<l n="d">jīvas taula-gr̥he tadā sa bhagavān sthāneśvaraḥ <lb n="S10"/> sthāpitaḥ <g type="ddanda">.</g></l>
</lg>
                    <lg n="10" met="anuṣṭubh" xml:lang="san-Latn">
                       <l n="a">ye haranti narā lobhā<space/>d</l>
<l n="b">deva-dravyāṇi mohitāḥ<space/></l>
<l n="c">vasanti narake loke<space/></l>
<l n="d">yāvac candra-divākarau <g type="ddanda">.</g></l>
</lg>
                    <lg n="11" met="anuṣṭubh" xml:lang="san-Latn">
                       <l n="a">mr̥tāmr̥tāś ca jīvanti<space/></l>
<lb n="S11"/><l n="b"><seg met="=="><gap reason="lost" quantity="2" unit="character"/></seg>y<seg type="component" subtype="body"><gap reason="illegible" quantity="1" unit="component"/></seg>ś ca punar-mr̥tāḥ<space/></l>
<l n="c">kṣuttr̥ṣṇā-ghātagharmmādyai<space/>r</l>
<l n="d">dduẖkhair bahu-vidhair hatāḥ <g type="ddanda">.</g></l>
</lg>
                    <lg n="12" met="anuṣṭubh" xml:lang="san-Latn">
                       <l n="a">Avīcī<supplied reason="lost">ṁ</supplied> prathamaṁ yānti<space/></l>
<l n="b">mahārauravam uttaram·<space/></l>
<lb n="S12"/><l n="c"><seg type="component" subtype="body"><gap reason="illegible" quantity="1" unit="component"/></seg>r̥<seg met="="><gap reason="lost" quantity="1" unit="character"/></seg>yakuṁ<unclear>bh</unclear>ipākāntaṁ<space/></l>
<l n="d">Ayaskaṇṭakitaṁ punaḥ <g type="ddanda">.</g></l>
</lg>
                    <lg n="13" met="anuṣṭubh" xml:lang="san-Latn">
                       <l n="a">śilāsaṁghātakaṁ yānti<space/></l>
<l n="b">Asipatravanaṁ gatāḥ<space/></l>
<l n="c">parvvatān· kṣuradhārāṁś ca<space/></l>
<l n="d">varccaskuṁ<unclear>bha</unclear>ṁ<lb n="S13"/><seg met="-+-"><gap reason="lost" quantity="3" unit="character"/></seg><unclear>tāḥ</unclear> <g type="ddanda">.</g></l>
</lg>
                    <lg n="14" met="anuṣṭubh" xml:lang="san-Latn">
                       <l n="a">sva-dattā<unclear>ṁ</unclear> para-dattāṁ vā<space/></l>
<l n="b">yo hareta vasundharāṁ<space/></l>
<l n="c">śva-viṣṭhāyāṁ krimir bhūtvā<space/></l>
<l n="d">pitr̥bhis saha pacyate <g type="ddanda">.</g></l>
</lg>
                   <lg n="15" met="anuṣṭubh" xml:lang="san-Latn">
                      <l n="a">deva-dravya-vināśā yo<space/></l>
<lb n="S14"/><l n="b"><unclear>vra</unclear>hma<supplied reason="lost">sva</supplied>haraṇena ca<space/></l>
<l n="c">kulānya-kulatāṁ yānti<space/></l>
<l n="d">brāhmaṇātikrameṇa ca <g type="ddanda">.</g></l>
</lg>
                   <lg n="16" met="anuṣṭubh" xml:lang="san-Latn">
                      <l n="a">marttyaloke jajāyante<space/></l>
<l n="b">dāsa-vargga-kuleṣu ca<space/></l>
<l n="c">daridrā rogino din<unclear>ā</unclear>ḥ<space/></l>
<lb n="S15"/><l n="d"><unclear>duẖkhair bahu-vidhair</unclear> hatāḥ <g type="ddanda">.</g></l>
</lg>

         	<ab>
         	    <lb n="1"/>6 roca puṣya candra<lb n="2" 
         	        break="no"/>vāra uttaraphalguni 
         	    <lb n="3"/>nakṣatra madhyā<lb n="4" 
         	        break="no"/>hna yoga vraḥ liṅga
         	</ab>

         <div type="edition" xml:lang="san-Latn">
             <lg n="1" met="anuṣṭubh"> 
<l n="a"><lb n="1"/><seg met="=="><gap reason="lost" quantity="2" unit="character"/></seg>tra<unclear>vā</unclear>ṅ-ruṅ<gap reason="illegible" quantity="1" unit="character"/><unclear>bhā</unclear>ge</l>
    <l n="b">bhū<unclear>mi</unclear>ḥ <seg met="==-+-="><gap reason="lost" quantity="6" unit="character"/></seg></l> 
<l n="c" enjamb="yes"><lb n="2"/><unclear>pa</unclear>digas-trapu-rai-dhānya</l>
    <l n="d">kuntikābhis sa-dakṣiṇāḥ <unclear><g type="ddandaOrnate">.</g></unclear></l>
</lg>
<lg n="2" met="anuṣṭubh">
<l n="a"><lb n="3"/>travāṅ-paśān-bhāge bhū<unclear>mi</unclear><supplied reason="omitted">ḥ</supplied></l>
    <l n="b">ra<unclear>m</unclear>ya-grāmasya gopa<unclear>k</unclear>aiḥ</l>
<l n="c" enjamb="yes"><lb n="4"/>padigas-sāṅguli-toṅ-sneṅ</l>
    <l n="d">rūpya-rai-trapu-dhānya<unclear>k</unclear>aiḥ <g type="ddandaOrnate">.</g></l>
</lg>
<lg n="3" met="anuṣṭubh">
<l n="a"><lb n="5"/>kumāra-candrasya bhāge</l>
    <l n="b">bhūmir go-vastra-<unclear>s</unclear>ā<unclear>ja</unclear>kaiḥ</l>
<l n="c"><lb n="6"/>kuntikā-dhānya-rai-rūpyaiḥ</l>
    <l n="d">trapubh<supplied reason="omitted">i</supplied>s tri-tulādibhiḥ <g type="ddandaOrnate">.</g></l>
</lg>
<lg n="4" met="anuṣṭubh">
<l n="a"><lb n="7"/>kriyārccana-catus-nehān·</l>
    <l n="b">rājatān hāra-kāñ<supplied reason="omitted">c</supplied>anān·</l> 
<l n="c" enjamb="yes"><lb n="8"/>dāsa-dāsī-kadāhādi</l>
    <l n="d">padigas-sa-kamandalūn· <g type="ddandaOrnate">.</g></l>
</lg>
<lg n="5" met="anuṣṭubh">
<l n="a"><lb n="9"/>saṁkrānte tandulaṁ droṇaṁ</l>
    <l n="b">śrī-jalāṅgeśvare ’diśat·</l>
<l n="c"><lb n="10"/>dvi-hasta-liṅge vighneśe</l>
    <l n="d">tat-padya-dva-dine sadā <g type="ddandaOrnate">.</g></l>
</lg>
<lg n="6" met="anuṣṭubh">
<l n="a"><lb n="11"/>sarvveṣāṁ bhūmi-bhāgā<choice><orig>ṇ</orig><reg>n</reg></choice>āṁ</l> 
    <l n="b">gola-sīmāvadhīṁ dadhe</l>
<l n="c" enjamb="yes"><lb n="12"/>tāsāṁ bhāge tu dhānyādi</l>
    <l n="d">kalpitāni śivasya <unclear>vai</unclear> <g type="ddandaOrnate">.</g></l>
</lg>
<lg n="7" met="anuṣṭubh">
<l n="a"><lb n="13"/>tai-kansān·qgat-trasul-joṅ-vraḥ</l>
    <l n="b">dāsīḥ hautra-samanvitāḥ</l>
<l n="c" enjamb="yes"><lb n="14"/>sī-kaṁvit·-thkal-panlas<unclear>·</unclear>-chke-vā</l>
    <l n="d">s-kaṁvau-dāsān sa-bhūmikān· <g type="ddandaOrnate">.</g></l>
</lg>
<lg n="8" met="anuṣṭubh">
<l n="a"><lb n="15"/>phala-priya-tapasvī yaḥ</l>
<l n="b">kantāl-sruk·snaṅ-purī-girau</l>
<l n="c" enjamb="yes"><lb n="16"/>yājako ’dhyāpakas sarvvā</l>
<l n="d">n dadau liṅge dvi-hastake <g type="ddandaOrnate">.</g></l>
</lg>
<lg n="9" met="anuṣṭubh">
<l n="a"><lb n="17"/>dāsā na preritās sarvve</l> 
    <l n="b">mukhendra-bhagavaj-janaiḥ</l>
<l n="c" enjamb="yes"><lb n="18"/>pālitās te kulenādri</l>
    <l n="d">patībhiḥ preritā navaiḥ <g type="ddandaOrnate">.</g></l>
</lg>
<lg n="10" met="anuṣṭubh">
<l n="a"><lb n="19"/>kalpitāni vinaśyanti</l>
    <l n="b">ye vilaṅghita-śāsanam·</l>
<l n="c"><lb n="20"/>yair amutreha te yānti</l>
    <l n="d">yātanā-narakān sadā <g type="ddandaOrnate">.</g></l>
</lg>
<lg n="11" met="anuṣṭubh">
<l n="a"><lb n="21"/>kalpitāni vivarddhante</l> 
    <l n="b">ye nu kārita-śāsanam·</l>
<l n="c"><lb n="22"/>yair amutreha te yānti</l> 
    <l n="d">vipulān paramāṅ gatim· <g type="ddandaOrnate">.</g></l>
</lg>
<lg n="12" met="anuṣṭubh">
<l n="a" enjamb="yes"><lb n="23"/>tri-pādi-kāśikā-harṣa</l>
    <l n="b">vaiśeṣika-mahotsavān·</l>
<l n="c" enjamb="yes"><lb n="24"/>tri-pārameśvara-<choice><orig>ṇ</orig><reg>n</reg></choice>yāyā</l>
    <l n="d">rāmāya<choice><orig>n</orig><reg>ṇ</reg></choice>a-sa-bhāratān· <g type="ddandaOrnate">.</g></l>
</lg>

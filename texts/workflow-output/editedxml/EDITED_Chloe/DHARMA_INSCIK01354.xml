            <div type="edition" xml:lang="okz-Latn">
                <p>
                    <milestone unit="face" n="A"/> 
                    <lb n="A1"/><num value="856">856</num> <unclear><g type="ddandaPlain">.</g></unclear> śaka <space quantity="1" unit="character"/> nu <choice><sic>mnā</sic><corr>mān</corr></choice> vraḥ śā<unclear>sa</unclear><supplied reason="lost">na dhūli vraḥ pāda</supplied>
                    <lb n="A2"/><unclear>dhū</unclear>li jeṅa vraḥ kaṁmrateṅ<unclear>∙</unclear> Añ∙ śri yaśo<supplied reason="lost">varmmadeva</supplied> <supplied reason="lost" cert="low">nu vraḥ kaṁmrateṅ Añ śrī</supplied>
                    <lb n="A3"/>harṣavarmmadeva ta steñ∙ Amarāvati <gap reason="lost" extent="unknown" unit="character"/> 
                    <lb n="A4"/>ta Āñ∙ stapa<unclear>se</unclear> kas<unclear>a</unclear> mra<add place="below">tā</add><supplied reason="omitted">ñ∙</supplied> śri <gap reason="illegible" quantity="1" unit="character"/>vās tra vikra<supplied reason="lost" cert="low">ma</supplied> <gap reason="lost" extent="unknown" unit="character"/> 
                    <lb n="A5"/>ma <unclear>vāla</unclear>ñ∙ <gap reason="illegible" quantity="2" unit="character"/> dauva ti pre Oy<unclear>∙</unclear> sruk<unclear>∙</unclear> <gap reason="lost" extent="unknown" unit="character"/> 
                    <lb n="A6"/>ṅa je ta mā <gap reason="lost" quantity="1" unit="character"/> kanmya<supplied reason="omitted">ṅ·</supplied> paṁmre daṁr<supplied reason="omitted">i</supplied>ṅ∙ vra<unclear>ḥ</unclear> <gap reason="lost" extent="unknown" unit="character"/> 
                    <lb n="A7"/><gap reason="illegible" quantity="1" unit="character"/> <unclear>kā</unclear> nu ta paṁpāta mula gi pu<unclear>n</unclear>ya śrai ya <gap reason="lost" extent="unknown" unit="character"/>
               </p>
                <p>
                    <milestone unit="face" n="B"/>
                    <lb n="B1"/>vraḥ kamrate<supplied reason="lost">ṅ∙ Añ∙ śi</supplied>vapāda ka<unclear>l</unclear>pa<supplied reason="lost">nā</supplied> <gap reason="lost" extent="unknown" unit="character"/> 
                    <lb n="B2"/>nā vāp∙ śiva <gap reason="lost" quantity="7" unit="character" precision="low"/> <unclear>la</unclear> <gap reason="lost" extent="unknown" unit="character"/> 
                    <lb n="B3"/><gap reason="lost" quantity="2" unit="character"/> kñuṁ toy∙ <supplied reason="lost">khnet</supplied> <gap reason="lost" quantity="4" unit="character"/> <unclear>pa</unclear> <gap reason="lost" extent="unknown" unit="character"/> 
                    <lb n="B4"/>pi <unclear>Ame</unclear> bhāja <unclear>to</unclear>y∙ <unclear>r</unclear>ṇ<supplied reason="lost">oc∙</supplied> <gap reason="lost" quantity="2" unit="character"/> <g type="dandaPlain">.</g> <g type="circleTarget">.</g> <unclear><g type="dandaPlain">.</g></unclear> <gap reason="lost" extent="unknown" unit="character"/>
                    <lb n="B5"/><gap reason="lost" quantity="12" unit="character" precision="low"/> na <gap reason="illegible" quantity="1" unit="character"/> <gap reason="lost" extent="unknown" unit="character"/> 
                   <lb n="B6"/><gap reason="lost" quantity="3" unit="character" precision="low"/>
                </p>
